{
  "uid": "web-performance",
  "title": "웹서버 성능 지표",
  "tags": ["neworbit", "web"],
  "timezone": "Asia/Seoul",
  "refresh": "30s",
  "time": { "from": "now-1h", "to": "now" },
  "panels": [
    {
      "id": 1,
      "title": "현재 접속자 수",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "chat_active_connections",
          "legendFormat": "접속자"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 10 },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 100 }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "대기열 인원",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "chat_waiting_queue_length",
          "legendFormat": "대기 중"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "누적 매칭 수",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "chat_matches_total",
          "legendFormat": "{{type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "purple", "value": null }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "누적 메시지 수",
      "type": "stat",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "chat_messages_total",
          "legendFormat": "메시지"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "접속자 수 추이",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "chat_active_connections",
          "legendFormat": "접속자"
        },
        {
          "expr": "chat_waiting_queue_length",
          "legendFormat": "대기열"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "fillOpacity": 20 }
        }
      }
    },
    {
      "id": 6,
      "title": "매칭 추이 (분당)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(chat_matches_total{type=\"human\"}[5m]) * 60",
          "legendFormat": "사람 매칭/분"
        },
        {
          "expr": "rate(chat_matches_total{type=\"ai\"}[5m]) * 60",
          "legendFormat": "AI 매칭/분"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "fillOpacity": 20 }
        }
      }
    },
    {
      "id": 7,
      "title": "메시지 처리량 (분당)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(chat_messages_total[5m]) * 60",
          "legendFormat": "메시지/분"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "fillOpacity": 20 }
        }
      }
    },
    {
      "id": 8,
      "title": "Nginx 활성 연결 수",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "nginx_connections_active",
          "legendFormat": "Active"
        },
        {
          "expr": "nginx_connections_waiting",
          "legendFormat": "Waiting"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "fillOpacity": 20 }
        }
      }
    },
    {
      "id": 9,
      "title": "Nginx 요청 처리량 (초당)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 22 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(nginx_connections_accepted[5m])",
          "legendFormat": "Accepted/s"
        },
        {
          "expr": "rate(nginx_connections_handled[5m])",
          "legendFormat": "Handled/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "fillOpacity": 20 }
        }
      }
    },
    {
      "id": 10,
      "title": "Node.js 이벤트루프 지연",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 22 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(nodejs_eventloop_lag_seconds[5m])",
          "legendFormat": "Event Loop Lag"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "fillOpacity": 20 }
        }
      }
    },
    {
      "id": 11,
      "title": "Node.js 힙 메모리",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 30 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "nodejs_heap_size_total_bytes",
          "legendFormat": "Heap Total"
        },
        {
          "expr": "nodejs_heap_size_used_bytes",
          "legendFormat": "Heap Used"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "fillOpacity": 20 }
        }
      }
    }
  ],
  "schemaVersion": 39
}
